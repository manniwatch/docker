{
  "name": "@manniwatch/docker",
  "version": "0.1.0",
  "description": "Server Helper helper for TrapezeApi",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "keywords": [
    "api",
    "types"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.app.json",
    "build:docker": "docker build . -t manniwatch/manniwatch",
    "build:readme": "npx @appnest/readme generate --input ../package_readme_blueprint.md --config readme_config.json --silent true",
    "start": "node ./dist/index.js",
    "start:docker": "docker run --rm manniwatch/manniwatch",
    "test": "mocha --config ../../.mocharc.yml",
    "test:coverage": "nyc --nycrc-path ../../.nycrc.json npm run test",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "postlint": "prettier --config .prettierrc src/**/*.ts src/*.ts --check",
    "lint:fix": "eslint -c .eslintrc.js --ext .ts ./src --fix",
    "postlint:fix": "prettier --config .prettierrc src/**/*.ts src/*.ts --write",
    "docs": "typedoc --options ../../typedoc.json",
    "prepublishOnly": "npm run test && npm run docs && npm run build",
    "version": "npm run build:readme && git add README.md"
  },
  "homepage": "https://manniwatch.github.io/docker/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/manniwatch/docker/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manniwatch/docker.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@manniwatch/api-proxy-server": "~0.27.2",
    "@manniwatch/api-types": "~0.27.0",
    "express": "~4.17.1"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/eslint-plugin-tslint": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "apidoc": "0.28.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.5.1",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "prettier": "2.3.2",
    "supertest": "6.1.4",
    "typescript": "4.3.5"
  }
}
